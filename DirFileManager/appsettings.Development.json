{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    },
    "File": {
      "Path": "command.log",
      "Append": true,
      "MinLevel": "Information",
      "FileSizeLimitBytes": 1048576,
      "MaxRollingFiles": 5
    }
  },
  "ItemCommands": [
    {
      "OrderId": 1,
      "Type": "FileDelete",
      "Delay": 1,
      "Parameter1": "c:\\file1.txt"
    },
    {
      "OrderId": 2,
      "Type": "FileCopy",
      "Delay": 1,
      "Parameter1": "c:\\file.txt",
      "Parameter2": "c:\\fileDestiny.txt"
    },
    {
      "OrderId": 3,
      "Type": "FileMove",
      "Delay": 1,
      "Parameter1": "c:\\fileOld.txt",
      "Parameter2": "c:\\fileNew.txt"
    },
    {
      "OrderId": 4,
      "Type": "FileCompress",
      "Delay": 1,
      "Parameter1": "c:\\file.txt",
      "Parameter2": "c:\\file.zip",
      "Parameter3": "Fastest"
    },
    {
      "OrderId": 5,
      "Type": "FileUncompress",
      "Delay": 1,
      "Parameter1": "c:\\file.zip",
      "Parameter2": "c:\\DirectoryDestiny"
    },
    {
      "OrderId": 6,
      "Type": "DirectoryDelete",
      "Delay": 1,
      "Parameter1": "c:\\Directory"
    },
    {
      "OrderId": 7,
      "Type": "DirectoryCopy",
      "Delay": 1,
      "Parameter1": "c:\\Directory",
      "Parameter2": "c:\\DirectoryDestiny"
    },
    {
      "OrderId": 8,
      "Type": "DirectoryMove",
      "Delay": 1,
      "Parameter1": "c:\\DirectoryOld",
      "Parameter2": "c:\\DirectoryDestiny"
    },
    {
      "OrderId": 9,
      "Type": "FileDelete",
      "Delay": 1,
      "Parameter1": "c:\\Directory",
      "Parameter2": "c:\\file.zip",
      "Parameter3": "Fastest"
    }
  ]
}
